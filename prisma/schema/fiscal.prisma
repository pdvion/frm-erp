model FiscalObligation {
  id              String    @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  companyId       String    @map("company_id") @db.Uuid
  type            String    @db.VarChar(30) // SPED_FISCAL, EFD_REINF, ESOCIAL, DCTF, DIRF, ECF, ECD, GIA, SINTEGRA
  year            Int
  month           Int       // 1-12
  dueDate         DateTime  @map("due_date") @db.Date
  status          String    @default("PENDING") @db.VarChar(20) // PENDING, IN_PROGRESS, SUBMITTED, LATE, RECTIFIED
  submittedAt     DateTime? @map("submitted_at")
